{
  "name": "@diplodoc/cli",
  "version": "5.19.2",
  "description": "Make documentation using yfm-docs in Markdown and HTML formats",
  "keywords": [
    "markdown",
    "yandex",
    "docs",
    "yfm",
    "documentation",
    "tool",
    "tools",
    "generator"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:diplodoc-platform/cli.git"
  },
  "license": "MIT",
  "author": "Yandex Data UI Team <data-ui@yandex-team.ru>",
  "typings": "build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "node": "./build/index.js",
      "default": "./build/index.mjs"
    },
    "./lib/*": {
      "types": "./lib/*/index.d.ts",
      "node": "./lib/*/index.js",
      "default": "./lib/*/index.mjs"
    },
    "./package": "./package.json",
    "./manifest": "./build/manifest.json",
    "./mdit-plugins": "./build/mdit-plugins.js",
    "./arcadia-vcs": "./build/arcadia-vcs.js",
    "./github-vcs": "./build/github-vcs.js"
  },
  "main": "build/index.js",
  "bin": {
    "docs": "build/index.js",
    "yfm": "build/index.js"
  },
  "files": [
    "lib",
    "build",
    "assets",
    "schemas"
  ],
  "scripts": {
    "build": "npm run build:clean && node scripts/build.cli.js",
    "build:types": "tspc -p tsconfig.lib.json || true; tspc -p tsconfig.build.json || true",
    "build:clean": "rm -rf build lib",
    "e2e": "npm run build && cd tests && npm test",
    "prepublishOnly": "npm run lint && npm run build && npm run build:types",
    "start": "node build/index.js",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "regenerate-schemas": "scripts/regenerate-schemas.js",
    "lint": "lint update && lint",
    "lint:fix": "lint update && lint fix",
    "pre-commit": "lint update && lint-staged",
    "prepare": "husky || exit 0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@diplodoc/client": "^5.2.0",
    "@diplodoc/liquid": "^1.4.0",
    "@diplodoc/transform": "^4.65.3",
    "@diplodoc/translation": "^1.7.19",
    "chalk": "^4.1.2",
    "commander": "^12.0.0",
    "csp-header": "^5.2.1",
    "dependency-graph": "^1.0.0",
    "execa": "^9.5.2",
    "glob": "^10.4.5",
    "highlight.js": "^11.10.0",
    "js-yaml": "4.1.0",
    "katex": "0.16.9",
    "lodash": "4.17.21",
    "markdown-it": "^13.0.2",
    "markdown-it-deflist": "^2.1.0",
    "markdown-it-meta": "^0.0.1",
    "markdown-it-sup": "^1.0.0",
    "normalize-path": "3.0.0",
    "p-map": "4.0.0",
    "tapable": "2.2.1",
    "threads": "1.7.0",
    "ts-dedent": "2.2.0"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.758.0",
    "@aws-sdk/lib-storage": "^3.758.0",
    "@diplodoc/latex-extension": "^1.4.1",
    "@diplodoc/lint": "^1.3.3",
    "@diplodoc/mermaid-extension": "^1.3.5",
    "@diplodoc/openapi-extension": "^5.0.7",
    "@diplodoc/page-constructor-extension": "^0.13.0",
    "@diplodoc/search-extension": "^3.0.0",
    "@diplodoc/tsconfig": "^1.0.2",
    "@diplodoc/yfmlint": "^1.3.3",
    "@octokit/core": "4.2.4",
    "@types/async": "3.2.15",
    "@types/chalk": "2.2.0",
    "@types/html-escaper": "3.0.0",
    "@types/js-yaml": "4.0.9",
    "@types/json-stringify-safe": "5.0.3",
    "@types/lodash": "4.14.195",
    "@types/markdown-it": "13.0.9",
    "@types/mime-types": "2.1.4",
    "@types/node": "18.19.4",
    "@vitest/coverage-v8": "^3.2.1",
    "ajv": "^8.11.0",
    "async": "^3.2.4",
    "axios": "^1.7.8",
    "chokidar": "^4.0.1",
    "esbuild": "^0.23.1",
    "html-escaper": "^3.0.3",
    "husky": "8.0.3",
    "mime-types": "2.1.35",
    "minimatch": "9.0.3",
    "node-html-parser": "6.1.5",
    "simple-git": "3.22.0",
    "slugify": "1.6.5",
    "svgo": "3.3.2",
    "ts-patch": "3.3.0",
    "typescript": "5.4.5",
    "typescript-transform-paths": "3.5.5",
    "uuid": "11.0.4",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.1",
    "vitest-when": "^0.6.1",
    "walk-sync": "3.0.0"
  },
  "engines": {
    "node": ">=22.*",
    "npm": ">=11.5.1"
  }
}
